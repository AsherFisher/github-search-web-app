<body class="container">
<div class="left">
  <p>Hi <%= name %></p>
<form action="/logout?_method=DELETE" method="POST">
  <button type="submit" class="btn btn-secondary">Log Out</button>
</form>
</div>
<img class ="aligncenter" src="/img/GitHub_logo.png" alt="Github logo">
<h1>Github search</h1>
<div class="input-group mb-3">
  <input id="input" type="text" class="form-control" placeholder="Enter a Github User Name...">
  <div class="input-group-append">
    <button id="search" class="btn btn-outline-secondary" type="button">Search</button>
  </div>
</div>
<div id="error" class="alert alert-danger"></div>

<button type="button" id="save" class="btn btn-success button">Save</button>
<button type="button" id="delete" class="btn btn-danger button">Delete</button>

<div class="center">
  <div id="data">
    <div id="Repositories"></div>
    <div id="followers"></div>
  </div>

  <div id="server">
  </div>

</div>
